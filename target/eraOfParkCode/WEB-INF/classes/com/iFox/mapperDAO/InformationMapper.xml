<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.iFox.mapperDAO.InformationMapper">
    <!--void save(Information informationMapper);-->
    <insert id="save">
        INSERT INTO tbl_info (userName,imgPath,sendTime,content)
        VALUES (#{userName},#{imgPath},#{sendTime},#{content})
    </insert>


</mapper>